{
  "name": "legacy-extends",
  "version": "1.0.0",
  "description": "Helper to extend a class including defining super_ and superConstruct (optional) on the derived class. Compatible with Node.js inherits and handles es6 classes using Reflect API",
  "keywords": [
    "extends",
    "extend",
    "inherits",
    "constructor",
    "super",
    "superConstruct",
    "ensureProperties",
    "Reflect",
    "reflect",
    "construct",
    "class",
    "classes",
    "babel",
    "es3",
    "es5"
  ],
  "homepage": "https://github.com/kmalakoff/legacy-extends",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/legacy-extends.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "dtd \"npm run lint\" \"depcheck\"",
    "test": "mocha-compat test/spec/**/*.test.js"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "depcheck": "^0.9.2",
    "dis-dat": "^0.1.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.0.5",
    "semver-compare": "^1.0.0"
  },
  "engines": {
    "node": ">=0.8.0"
  }
}
