{
  "name": "legacy-extends",
  "version": "1.0.12",
  "description": "Helper to extend a class including defining super_ and superConstruct (optional) on the derived class. Compatible with Node.js inherits and handles es6 classes using Reflect API",
  "keywords": [
    "extends",
    "extend",
    "inherits",
    "constructor",
    "super",
    "superConstruct",
    "ensureProperties",
    "Reflect",
    "reflect",
    "construct",
    "class",
    "classes",
    "babel",
    "es3",
    "es5"
  ],
  "homepage": "https://github.com/kmalakoff/legacy-extends",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/legacy-extends.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "semver-compare": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
